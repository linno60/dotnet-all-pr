sionIDManager::GetSessionID","System.Web.SessionState.SessionIDManager.Initialize","System::Web::SessionState::SessionIDManager::Initialize","System.Web.SessionState.SessionIDManager.InitializeRequest","System::Web::SessionState::SessionIDManager::InitializeRequest","System.Web.SessionState.SessionIDManager.RemoveSessionID","System::Web::SessionState::SessionIDManager::RemoveSessionID","System.Web.SessionState.SessionIDManager.SaveSessionID","System::Web::SessionState::SessionIDManager::SaveSessionID","System.Web.SessionState.SessionIDManager.SessionIDMaxLength","System::Web::SessionState::SessionIDManager::SessionIDMaxLength","System.Web.SessionState.SessionIDManager.Validate","System::Web::SessionState::SessionIDManager::Validate"],"dev_langs":["csharp"],"updated_at":"2017-03-22 03:41 AM","document_id":"63d68802-6891-ddf4-adac-a2b019fb2e7f","content_git_url":"https://github.com/TianqiZhang/ECMA2YamlTestRepo2/blob/master/xml/System.Web.SessionState/SessionIDManager.xml","layout":"Reference","_op_layout":"Reference","pagetype":"Reference","title":"SessionIDManager class | Microsoft Docs","_op_ogTitle":"SessionIDManager class","description":"Manages unique identifiers for ASP.NET session state.\n","toc_asset_id":"api/_splitted/System.Web.SessionState/toc.json","toc_rel":"_splitted/System.Web.SessionState/toc.json","source_url":"","ms.assetid":"System.Web.SessionState.SessionIDManager","canonical_url":"https://docs.microsoft.com/en-us/dotnet-all/api/system.web.sessionstate.sessionidmanager","_op_searchScope":[".NET"],"_op_canonicalUrl":"https://docs.microsoft.com/en-us/dotnet-all/api/system.web.sessionstate.sessionidmanager","fileRelativePath":"api/System.Web.SessionState.SessionIDManager.html"},"themesRelativePathToOutputRoot":"_themes/"}